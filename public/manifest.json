{
  "manifest_version": 3,
  "name": "BookmarkNav",
  "version": "1.0.0",
  "description": "A Zen-inspired bookmark manager.",
  "action": {
    "default_popup": "index.html",
    "default_title": "BookmarkNav",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "bookmarks"
  ],
  "host_permissions": [
    "https://www.google.com/"
  ]
}